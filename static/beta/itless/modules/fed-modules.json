{
    "landing": {
        "manifestLocation": "/apps/landing/fed-mods.json",
        "defaultDocumentTitle": "Hybrid Cloud Console Home",
        "modules": [
            {
                "id": "landing",
                "module": "./RootApp",
                "routes": [
                    {
                        "exact": true,
                        "pathname": "/"
                    }
                ]
            }
        ]
    },
